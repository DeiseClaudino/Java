public class Conta {
	double saldo;
	int agencia =42;
	int numero;
	String titular;
	
	public void deposita(double valor) {
		//saldo = saldo + valor
		this.saldo += valor;
	}
	
	public boolean saca(double valor) {
		if(this.saldo>= valor) {
			this.saldo-= valor;
			
			return true;
			System.out.println(true);
		}else {
			return false;
		}
		
		
	}
}